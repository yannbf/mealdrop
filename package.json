{
  "name": "course-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hooks-helper": "^1.6.0",
    "react-loading-skeleton": "^2.2.0",
    "react-lottie-player": "^1.3.1",
    "react-multi-carousel": "^2.6.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5",
    "use-dark-mode": "^2.3.1"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o build/storybook",
    "build-all": "yarn build && yarn build-storybook",
    "chromatic": "chromatic --project-token $CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes --only-changed",
    "prepare": "husky install",
    "lint": "eslint --fix .",
    "prettier": "prettier --write .",
    "pre-commit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-coverage": "^0.0.2",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-storyshots": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/test-runner": "^0.5.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^15.12.3",
    "@types/react-dom": "^17.0.11",
    "@types/react-hooks-helper": "^1.6.3",
    "@types/react-redux": "^7.1.20",
    "@types/react-transition-group": "^4.4.4",
    "@types/styled-components": "^5.1.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-istanbul": "^0.25.1",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-open-source": "^1.3.3",
    "c8": "^7.12.0",
    "chromatic": "^6.1.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": ">=6",
    "istanbul": "^0.4.5",
    "jest-canvas-mock": "^2.3.1",
    "jest-junit": "^13.2.0",
    "jsdom": "^20.0.2",
    "lint-staged": ">=10",
    "msw": "^0.36.0",
    "msw-storybook-addon": "^1.6.3",
    "prettier": "^2.5.0",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-designs": "^6.3.1",
    "storybook-mobile": "^1.0.0",
    "vite": "^3.2.3",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.1",
    "vitest-axe": "^0.1.0",
    "vitest-canvas-mock": "^0.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{html,md}": "prettier --write"
  }
}
