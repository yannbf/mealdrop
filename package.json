{
  "name": "course-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.3",
    "@types/react-dom": "^17.0.8",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hooks-helper": "^1.6.0",
    "react-loading-skeleton": "^2.2.0",
    "react-lottie-player": "^1.3.1",
    "react-multi-carousel": "^2.6.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^4.0.3",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.4",
    "use-dark-mode": "^2.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --setupFiles ./src/setup.ts",
    "test:ci": "yarn test --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "build-all": "yarn build && yarn build-storybook",
    "chromatic": "chromatic --project-token $CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes --only-changed",
    "prepare": "husky install",
    "lint": "eslint --fix .",
    "prettier": "prettier --write .",
    "pre-commit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-a11y": "^6.4.0-beta.26",
    "@storybook/addon-essentials": "^6.4.0-beta.26",
    "@storybook/addon-interactions": "^6.4.0-beta.12",
    "@storybook/addon-storyshots": "^6.4.0-beta.26",
    "@storybook/jest": "^0.0.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.4.0-beta.26",
    "@storybook/testing-library": "^0.0.3",
    "@storybook/testing-react": "^1.0.0-next.0",
    "@types/jest-axe": "^3.5.1",
    "@types/react-hooks-helper": "^1.6.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-transition-group": "^4.4.1",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-loader": "8.1.0",
    "chromatic": "^6.0.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=6",
    "jest-axe": "^4.1.0",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": ">=10",
    "msw": "^0.29.0",
    "msw-storybook-addon": "^1.4.1",
    "prettier": "^2.3.2",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-designs": "^6.2.0",
    "storybook-mobile": "^0.1.32"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{html,md}": "prettier --write"
  }
}
