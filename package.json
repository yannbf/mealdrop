{
  "name": "course-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hooks-helper": "^1.6.0",
    "react-loading-skeleton": "^2.2.0",
    "react-lottie-player": "^1.3.1",
    "react-multi-carousel": "^2.6.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.9.5",
    "use-dark-mode": "^2.3.1"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir build/storybook",
    "build-storybook:test": "storybook build --output-dir build/storybook --test",
    "build-all": "yarn build && yarn build-storybook",
    "chromatic": "chromatic --project-token $CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes --only-changed",
    "prepare": "husky install",
    "lint": "eslint --fix .",
    "prettier": "prettier --write .",
    "pre-commit": "lint-staged",
    "test-storybook": "test-storybook",
    "test-storybook:json": "test-storybook --index-json",
    "test-storybook:ci": "concurrently --kill-others --success first --names \"SB,TEST\" --prefix-colors \"magenta,blue\" \"yarn build-storybook:test && npx serve build/storybook --listen 6006 --no-request-logging\" \"wait-on tcp:6006 && yarn test-storybook\"",
    "test-storybook:json:ci": "concurrently --kill-others --success first --names \"SB,TEST\" --prefix-colors \"magenta,blue\" \"yarn build-storybook:test && npx serve build/storybook --listen 6006 --no-request-logging\" \"wait-on tcp:6006 && SKIP_SNAPSHOTS=true yarn test-storybook:json\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@chromatic-com/storybook": "^1.2.18",
    "@ladle/react": "^2.4.5",
    "@storybook/addon-a11y": "^8.5.0-beta.1",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-designs": "8.0.0-next.1",
    "@storybook/addon-essentials": "^8.5.0-beta.1",
    "@storybook/addon-themes": "^8.5.0-beta.1",
    "@storybook/blocks": "^8.5.0-beta.1",
    "@storybook/experimental-addon-test": "^8.5.0-beta.1",
    "@storybook/manager-api": "^8.5.0-beta.1",
    "@storybook/react": "^8.5.0-beta.1",
    "@storybook/react-vite": "^8.5.0-beta.1",
    "@storybook/test": "^8.5.0-beta.1",
    "@storybook/test-runner": "next",
    "@storybook/theming": "^8.5.0-beta.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^15.12.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-hooks-helper": "^1.6.3",
    "@types/react-redux": "^7.1.20",
    "@types/react-transition-group": "^4.4.4",
    "@types/styled-components": "^5.1.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/browser": "^2.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-open-source": "^1.3.3",
    "c8": "^7.12.0",
    "chromatic": "^6.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": ">=6",
    "istanbul": "^0.4.5",
    "jest-junit": "^13.2.0",
    "jsdom": "^20.0.2",
    "lint-staged": ">=10",
    "msw": "^1.3.2",
    "msw-storybook-addon": "^1.10.0",
    "playwright": "^1.49.1",
    "prettier": "^2.5.0",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "storybook": "^8.5.0-beta.1",
    "vite": "^4.0.0",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^2.1.0",
    "vitest-axe": "^0.1.0",
    "vitest-canvas-mock": "^0.2.2",
    "wait-on": "^7.2.0"
  },
  "resolutions": {
    "@types/react": "^17.0.2"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{html,md}": "prettier --write"
  },
  "packageManager": "yarn@4.1.1"
}
